    "@angular/router": "~12.0.3",
    "codejar": "^3.4.0",
    "highlight.js": "^11.0.1",
    "prismjs": "^1.23.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
